<h3>Listing admins</h3>

<table>
  <tr>
    <th>Full Name</th>
    <th>Email</th>
    <th>Company</th>
    <th>League ID</th>
  </tr>


<% @admins.each do |admin| %>
  <tr>
    <td><%= link_to admin.name, edit_admin_path(admin) %></td>
    <td><%= admin.email %></td>
    <td><%= admin.company_name %></td>
    <td><%= admin.league_id %></td>
    <td><%= link_to 'Show', admin %></td>
  </tr>
<% end %>
</table>
<br>
<%= link_to 'Update Admins', :controller => 'admins', :method => :get, :action => 'update_admins' %>
