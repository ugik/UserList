<p>
  <b>Name:</b>
  <%= @admin.name %><b> Email:</b>  <%= @admin.email %><br>
  <b>Company:</b>  <%= @admin.company_name %>  <b>League ID:</b>  <%= @admin.league_id %>
</p>

<% unless @admin.challenges.empty? %>
  <b>Challenges</b>
  <table class="challenges" summary="league challenges">
  <%= render @challenges %>
  </table>
<% end %>

<% if @show_graph %>
	<% option = { :width => 650, :height => 240, :title => 'Registrations' }
	@chart = GoogleVisualr::Interactive::AreaChart.new(@data_table, option) %>
	<div id='chart'></div>
	<%= render_chart(@chart, 'chart') %>
<% end %>
